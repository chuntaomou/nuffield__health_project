                  button#search-locations-button(style='width:166px; height:58px; background-color:#00a200; font-size:20px; color:#fff;') Search
                .field.f-filter-1
                  select#s_Region.filterSelect1(name='s_Region')
                    option#1 Search by location name
                    option#2(selected='selected') Search by location code
                .field.f-filter-2
                  select#s_Category.filterSelect1(name='s_Category')
                    option#1(selected='selected') Fitness and Wellbeing Centre
                    option#2 Cluster
                    option#3 Region
                    option#4 Sales Channel
                    option#5 Hospital
                    option#6 Site
                    option#9 Any
