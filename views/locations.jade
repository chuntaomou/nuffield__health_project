  body.page-our-locations.navlevel-3.doctype-locationslandingpage(style='background-color: #f1f1f1;')
    // Skip links - Accessibility
    ul.skip-links.visuallyhidden
      li
        a(href='#main') Skip to content
      li
        a(href='#navigation-primary') Main site navigation
      li
        a(href='#footer') Share links and site information
    #top.container(style='min-width:1700px;')
      // Modal for association
      #addlocationModal.modal.fade(role='dialog')
        .modal-dialog
          // Modal content
          .modal-content
            .modal-header
              button.close(type='button', data-dismiss='modal') Ã—
              h4.modal-title Add Location
            .modal-body
              input#modal-location-type-id(style='display:none;')
              input#modal-association-type-id(style='display:none;')
              input#modal-association-child-product-id(style='display:none;')
              .dropdown(style='background-color:#fff;')
                label(style='min-width:100px;')
                  strong Location Type:
                // 
                input#location-type-name(style='margin-left: 40px; max-height:34px;')
                // 
                button.btn.btn-default.dropdown-toggle(data-toggle='dropdown', style='margin-left: 0px;')
                  | View
                  span.caret
                ul#modal-location-dropdown.dropdown-menu(style='margin-left:380px; width:200px; height:390px;')
                  li
                    a#1.location-type-name Fitness and Wellbeing Centre
                  li
                    a#2.location-type-name Cluster
                  li
                    a#3.location-type-name Region
                  li
                    a#4.location-type-name Sales Channel
                  li
                    a#5.location-type-name Hospital
                  li
                    a#6.location-type-name Site
              .row
                label(style='min-width:100px;')
                  strong Location Name:
                // 
                input#location-name(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:100px;')
                  strong Location Code:
                // 
                input#location-code(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:100px;')
                  strong Location Label:
                // 
                input#location-label(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row(style='margin-top:20px;')
                label(style='min-width:158px;')
                  strong Location External Code:
                // 
                input#location-external-code(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:158px;')
                  strong Location External Id:
                // 
                input#location-external-id(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:158px;')
                  strong Location Description:
                // 
                textarea#location-description(rols='10', cols='45', style='margin-left: 40px; margin-top:4px;')
              .row(style='margin-top:20px;')
                label(style='font-weight:bold;', for='location-date-from') Location Attribute Valid From
                br
                input#location-date-from.form-control(style='width:100%;', type='text', placeholder='', title='format : ')
                br
                label(style='font-weight:bold;', for='location-date-to') Location Attribute Valid To
                br
                input#location-date-to.form-control(style='width:100%;', type='text', placeholder='', title='format : ')
                br
                script.
                  $('#location-date-from').datepicker({theme:'green'});
                  $('#location-date-to').datepicker({theme:'green'});
            .modal-footer
              button#add-new-location.btn.btn-danger Add
              button.btn.btn-default(type='button', data-dismiss='modal') Close
