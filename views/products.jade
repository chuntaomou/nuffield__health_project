  body.page-our-locations.navlevel-3.doctype-locationslandingpage(style='background-color: #f1f1f1;')
    // Skip links - Accessibility
    ul.skip-links.visuallyhidden
      li
        a(href='#main') Skip to content
      li
        a(href='#navigation-primary') Main site navigation
      li
        a(href='#footer') Share links and site information
    #top.container(style='max-width:1595px;')
      // Modal for association
      #addproductModal.modal.fade(role='dialog')
        .modal-dialog
          // Modal content
          .modal-content
            .modal-header
              button.close(type='button', data-dismiss='modal') Ã—
              h4.modal-title Add Product
            .modal-body
              input#modal-product-type-id(style='display:none;')
              input#modal-association-type-id(style='display:none;')
              input#modal-association-child-product-id(style='display:none;')
              .dropdown(style='background-color:#fff;')
                label(style='min-width:100px;')
                  strong Product Type:
                // 
                input#product-type-name(style='margin-left: 40px; max-height:34px;')
                // 
                button.btn.btn-default.dropdown-toggle(data-toggle='dropdown', style='margin-left: 0px;')
                  | View
                  span.caret
                ul#modal-product-dropdown.dropdown-menu(style='margin-left:380px; width:200px; height:360px;')
                  li
                    a#1.product-type-name Product
                  li
                    a#2.product-type-name Product Group
                  li
                    a#3.product-type-name Product Class
                  li
                    a#4.product-type-name Product Family
                  li
                    a#5.product-type-name VAT Code
                  li
                    a#6.product-type-name Product Brand
                  li
                    a#7.product-type-name Product Sub Brand
                  li
                    a#8.product-type-name Product Price Tier
              .row
                label(style='min-width:100px;')
                  strong Product Name:
                // 
                input#product-name(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:100px;')
                  strong Product Code:
                // 
                input#product-code(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:100px;')
                  strong Product Label:
                // 
                input#product-label(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row(style='margin-top:20px;')
                label(style='min-width:158px;')
                  strong Product External Code:
                // 
                input#product-external-code(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:158px;')
                  strong Product External Id:
                // 
                input#product-external-id(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:158px;')
                  strong Turn around days:
                // 
                input#product-turn-around-time-days(style='margin-left: 40px; max-height:34px; margin-top:4px;')
              .row
                label(style='min-width:158px;')
                  strong Product Description:
                // 
                textarea#product-description(rols='10', cols='45', style='margin-left: 40px; margin-top:4px;')
              .row(style='margin-top:20px;')
                label(style='font-weight:bold;', for='product-date-from') Product Attribute Valid From
                br
                input#product-date-from.form-control(style='width:100%;', type='text', placeholder='', title='format : ')
                br
                label(style='font-weight:bold;', for='product-date-to') Product Attribute Valid To
                br
                input#product-date-to.form-control(style='width:100%;', type='text', placeholder='', title='format : ')
                br
                script.
                  $('#product-date-from').datepicker({theme:'green'});
                  $('#product-date-to').datepicker({theme:'green'});
            .modal-footer
              button#add-new-product.btn.btn-danger Add
              button.btn.btn-default(type='button', data-dismiss='modal') Close
